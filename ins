def insertionsort(n,arr):
    count=0
    if arr==sorted(arr):
        return (0)    
    else:
        for i in range(1,len(arr)):
            x=arr[i]
            for j in range(i):
                if j>=0 and arr[j]>x:
                    (arr[i],arr[j])=(arr[j],arr[i])
                    count+=1
        
        return count
n = int(input())
arr = list(map(int, input().split()))
print (insertionsort(n,arr))
